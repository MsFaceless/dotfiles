#!/bin/python

from fastapi import FastAPI, Depends

from model.dbinit import get_db
from model.base_models import *

app = FastAPI(title="RocketModule", root_path="/rocket", version='0.0.1')

@app.get('/')
async def root():
    outputdict = {
        'success' : True,
        'data' : 'Welcome to the Rocket Gateway.',
    }
    return outputdict

@app.get('/hello_world', response_model=GenericResponse)
async def root(db=Depends(get_db)):
    outputdict = {
        'success' : True,
        'data' : {},
    }
    return outputdict
