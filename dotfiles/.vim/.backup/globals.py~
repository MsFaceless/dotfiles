import os

account_sid = os.environ["ACCOUNT_SID"]
print(account_sid)

auth_token = os.environ["AUTH_TOKEN"]
print(auth_token)

service_sid = os.environ["MESSAGING_SID"]
print(service_sid)

twillio_number = os.environ["TWILLIO_NUMBER"]
print(twillio_number)

whatsapp_number = os.environ["WHATSAPP_NUMBER"]
print(whatsapp_number)
