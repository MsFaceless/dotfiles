        javascript = """
        <script>
            $("#bank_id").change(function () {
                var bank_id = $('#bank_id option:selected').val();
                var kwargs = {'bank_id': bank_id, };
                $('#div_bank_branch').load('/members/get_selectbox_bank_branch', kwargs, function(data){
                    return false;
                });
            });
        </script>
        """
